{
  "name": "@nexusloop/db",
  "version": "0.0.1",
  "private": true,
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --no-config",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch --no-config",
    "lint": "eslint \"src/**/*.ts*\"",
    "clean": "rimraf .turbo node_modules dist",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:studio": "prisma studio"
  },
  "dependencies": {
    "@prisma/client": "^5.10.0",
    "@nexusloop/types": "0.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.11.30",
    "eslint": "^8.57.0",
    "prisma": "^5.10.0",
    "rimraf": "^5.0.5",
    "tsup": "^8.0.2",
    "typescript": "^4.9.5"
  }
}
